            }
         },
         "image": "https://i.ytimg.com/vi/1E784poGCAA/maxresdefault.jpg",
         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/06_23_20/06/23/18/URPRISGL/BJZX6O65.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
