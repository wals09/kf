         "image": "https://reyting-avto.ru/wp-content/uploads/2020/05/forsazh.jpg",
         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/09_21_22/09/21/23/PMZGY6JG/P2UWZ3Z6.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
         "properties": {
            "image:extension": "| {col:msx-white}First Item"
         }
