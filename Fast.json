         "image": "https://i.pinimg.com/736x/4f/89/92/4f8992b2032c68560a299927eec66b76--free-tv-show-paul-walker-movies.jpg",
         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/09_25_20/09/25/00/E4LKY4QT/ICESB53I.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
         "properties": {
            "image:extension": "| {col:msx-white}Second Item"
         }
