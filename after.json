            }
         },
         "image": "https://1.bp.blogspot.com/-nYqLdY_pEEU/XweBYBnl85I/AAAAAAAAiVE/GWp2TGcnC00C2J_yA-Sq_6RgI24525_IACLcBGAsYHQ/s640/after.jpg",
         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/07_26_23/07/26/19/N6W6424P/NF2TRZ2Q.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
         "properties": {
            "image:extension": "| {col:msx-white}First Item"
         }
