            }
         },
         "image": "https://i.ytimg.com/vi/zpcu5Yh6r_U/maxresdefault.jpg",
         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/10_03_20/10/03/01/DBEYEZQT/WRINHAEX.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
