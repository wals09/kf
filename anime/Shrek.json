         "properties": {
            "image:extension": "| {col:msx-white}First Item"
         }
      },
      {
         "titleHeader": "{col:msx-white}Шрэк",
         "titleFooter": "Шрэк 4-D",
         "selection": {
            "important": true,
            "action": "[update:content:overlay:image|update:content:overlay:text]",
            "data": {
               "text": "{col:msx-white}Шрэк 4-D{col}{br}Лорд Фаркуард был съеден драконом, но его дух все еще жив и все так же зол. Он похищает Фиону с помощью все еще живого помощника-подручного. Шрек и Осел отправляются в путешествие, чтобы спасти принцессу.",
               "image": "https://orlandohoppers.com/wp-content/uploads/2017/09/Shrek_4-D_0.jpg"
            }
         },
         "image": "https://orlandohoppers.com/wp-content/uploads/2017/09/Shrek_4-D_0.jpg",
         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/04_08_20/04/08/12/UIIZLAQ3/J6C6PMFB.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
         "properties": {
            "image:extension": "| {col:msx-white}Second Item"
