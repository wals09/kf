         "action": "video:resolve:request:interaction:https://hye1eaipby4w.takedwn.ws/01_30_19/01/30/04/ny9TRVtJ/1080_XegLjCtO.mp4/master.m3u8@http://msx.benzac.de/interaction/play.html",
         "properties": {
            "image:extension": "| {col:msx-white}Fourth Item"
